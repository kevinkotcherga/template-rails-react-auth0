<div id="<%= dom_id product %>">
  <p>
    <strong>Title:</strong>
    <%= product.title %>
  </p>

  <p>
    <strong>Desc:</strong>
    <%= product.desc %>
  </p>

  <p>
    <strong>Img:</strong>
    <%= product.img %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Instock:</strong>
    <%= product.instock %>
  </p>

</div>
